{
  "version": 2,
  "functions": {
    "api/**": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/login.html"
    },
    {
      "source": "/login",
      "destination": "/login.html"
    },
    {
      "source": "/index",
      "destination": "/index.html"
    },
    {
      "source": "/sw.js",
      "destination": "/sw.js"
    }
  ]
}